//Programa : RFID - Controle de Acesso leitor RFID//Autor : FILIPEFLOP #include "RingTone.h"#include "RFID.h"#define SS_PIN 10#define RST_PIN 9#define BUZZER 7RFID rfid(SS_PIN, RST_PIN); // instancia o leitorRingTone ringTone(BUZZER);void setup() {  rfid.startRead();  Serial.begin(9600);   // Inicia a serial  Serial.println("Aproxime o seu cartao do leitor...");  Serial.println();  pinMode(BUZZER, OUTPUT); // seta o pino onde está o buzzer como saida  } void loop() {  // caso não tenha lido nenhum cartão, quebra o  if(!rfid.hasCard()){    return;  }  //Mostra UID na serial  String cardID = rfid.getCardID();  Serial.print("UID da tag :");  Serial.println(cardID);    if (cardID.substring(1) == "F6 52 7E 8D") //UID 1 - Chaveiro  {    ringTone.success();    Serial.println("Acesso liberado!");  }else  {    ringTone.error();    Serial.println("Acesso negado!");  }    delay(1000);     }  